models:
    Account: { user_id: 'id foreign', website: 'string:50 required', username: 'string:50 nullable', email: 'string:50 nullable', account_password: 'string:50 nullable', password: 'string:50 nullable', relationships: { hasMany: 'Application, Mission' } }
