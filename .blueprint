    Course: { user_id: 'id foreign', name: 'string:50 required', link: 'string nullable', subject: 'string:50 nullable', level: 'enum:easy,medium,hard,difficult', chapters: 'nullable integer', length: 'nullable integer', progress: 'nullable integer' }
    Mission: { user_id: 'id foreign', account_id: 'id foreign', name: 'string:50 required', company: 'string:50 nullable', priority: 'enum:low,medium,high,important nullable', urgency: 'enum:slow,medium,fast,urgent nullable', submission_date: 'date nullable', status: 'enum:waiting,received,pending,submitted,accepted nullable', income: 'integer nullable', relationships: { belongsTo: Account, hasMany: Task } }
    Label: { name: 'required string:50', color: 'nullable string:50', relationships: { belongsToMany: Task } }
    Task: { user_id: 'id foreign', mission_id: 'id foreign', name: 'required string:50', description: 'nullable string:200', time: 'date nullable', status: 'boolean default:0', relationships: { belongsToMany: Label, belongsTo: 'User, Mission' } }
    TaskLabel: { task_id: 'id foreign', label_id: 'id foreign' }
    Subject: { subject: 'string:100 required', relationships: { hasMany: Course, belongsTo: 'Topic, Category' } }
    Topic: { topic: 'string:100 required', relationships: { hasMany: 'Course, Subject', belongsTo: Category } }
    Category: { category: 'string:100 required', relationships: { hasMany: 'Course, Topic, Subject' } }
